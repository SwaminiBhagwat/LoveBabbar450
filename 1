System.out.println("Original Array is ");
		   for(int i=0;i<arr.length;i++) {
			   System.out.print(arr[i]+" ");
		   }System.out.println();
		System.out.println("Reversed Array is :");
		for(int i=arr.length-1;i>=0;i--) {
			
